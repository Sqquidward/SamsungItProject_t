<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="140dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.cardview.widget.CardView

        android:layout_width="match_parent"
        android:layout_height="140dp"
        style="@style/CardView.Light"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/name_nase_ch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginTop="25dp"
                android:text="Bytcoin"
                android:textColor="@color/white"
                app:layout_constraintStart_toEndOf="@+id/imageView_nase"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="2dp"
                android:layout_height="18dp"
                android:layout_marginTop="25dp"
                android:background="@color/white"
                android:orientation="horizontal"
                app:layout_constraintEnd_toStartOf="@+id/id_nasm_ch"
                app:layout_constraintStart_toEndOf="@+id/name_nase_ch"
                app:layout_constraintTop_toTopOf="parent">

            </LinearLayout>

            <TextView
                android:id="@+id/id_nasm_ch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="25dp"
                android:text="BTC"
                android:textColor="@color/white"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/name_nase_ch"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView_nase"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="25dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="90dp"
                android:text="Market cap rank:"
                android:textColor="@color/white"
                android:textSize="10dp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView_nase"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/marketcaprank_nase_ch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="90dp"
                android:text="10203"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/textView10"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="60dp"
                android:text="Price:"
                android:textColor="@color/white"
                android:textSize="10dp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView_nase"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/price_nasm_ch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="60dp"
                android:text="10203"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/textView4"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="60dp"
                android:text="$"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/price_nasm_ch"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="250dp"
                android:layout_marginTop="60dp"
                android:text="High 24:"
                android:textColor="@color/white"
                android:textSize="10dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="225dp" />

            <TextView
                android:id="@+id/high24_nase_ch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="60dp"
                android:text="2342"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/textView6"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="275dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="60dp"
                android:text="$"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/high24_nase_ch"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="300dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="250dp"
                android:layout_marginTop="90dp"
                android:text="Low 24:"
                android:textColor="@color/white"
                android:textSize="10dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="225dp" />

            <TextView
                android:id="@+id/low_24_nasm_ch"
                android:layout_width="wrap_content"
                android:layout_height="14dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="90dp"
                android:text="2342"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/textView8"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="275dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="6dp"
                android:layout_height="13dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="90dp"
                android:text="$"
                android:textColor="@color/white"
                android:textSize="10dp"
                app:layout_constraintStart_toEndOf="@+id/low_24_nasm_ch"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_editor_absoluteX="300dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

</FrameLayout>